{
  "name": "@guillaumehanotel/vue-snowfall",
  "version": "2.4.2",
  "description": "A Vue component that creates a snowfall effect",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./vanilla": {
      "import": "./lib/vanilla.js",
      "require": "./lib/vanilla.cjs",
      "types": "./lib/vanilla.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "vue-tsc && vite build",
    "start": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "standard-version --infile ../../CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumehanotel/vue-snowfall.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "snowfall",
    "winter",
    "christmas",
    "canvas"
  ],
  "author": "Guillaume HANOTEL",
  "license": "MIT",
  "homepage": "https://github.com/guillaumehanotel/vue-snowfall#readme",
  "bugs": {
    "url": "https://github.com/guillaumehanotel/vue-snowfall/issues"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.2.4",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.4",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.4",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3"
  }
}
